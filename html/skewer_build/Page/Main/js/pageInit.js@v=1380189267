$(function(){

    var bDebug = false;

    if(bDebug){

        console.log('Document rendered: %s',new Date());
    }//is debug

    $('.js_use_resize').fancybox({
        openEffect: 'fade',
        closeEffect: 'fade',
        nextEffect: 'none',
        prevEffect: 'none',
        arrows    : true,
        nextClick : true,
        mouseWheel: true
    });


});
